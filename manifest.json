{
    "manifest_version":3,
    "name":"Symbols Extension",
    "version":"0.0.1",
    "description": "An extension that copies special characters to your clipboard.",
    "options_page": "settings_Basic.html",
    "action":{
        "default_popup": "popup.html"
    },
    "icons": { 
        "16": "test_Extension_Image.png",
        "32":"test_Extension_Image.png",
        "48": "test_Extension_Image.png",
       "128": "test_Extension_Image.png"
    },
    "background":{
        "service_worker":"worker.js"
    },
    "permissions":[
        "activeTab",
        "storage"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],       
          "js": ["content.js"]
        }
      ]
      
}

