{
    "manifest_version":3,
    "name":"Symbols Extension",
    "version":"0.0.1",
    "description": "An extension that copies special characters to your clipboard.",
    "options_page": "settingsPage/settingsBasic.html",
    "minimum_chrome_version": "46",
    "action":{
        "default_popup": "popup/popup.html",
        "default_title": "Accents and symbols MAKE THIS RELEVANT LATER"
    },
    "icons": {
        "16": "testImage.png",
        "32": "testImage.png",
        "48": "testImage.png",
       "128": "testImage.png"
    },
    "background":{
        "service_worker":"worker.js"
    },
    "permissions":[
        "activeTab",
        "storage",
        "scripting"
    ],
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
      ]

}
